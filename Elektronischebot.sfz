<control>
hint_ram_based=1
set_cc43=0
set_cc46=127
set_cc53=0
set_cc56=0
set_cc64=0
label_cc43=Attack
label_cc46=Decay
label_cc53=Sustain
label_cc56=Release
label_cc64=Hold (keep on)

<global>
polyphony=128 //check and make sure the polyphony is set to 128 in your software, it might default to 64

ampeg_attack_oncc43=2 //ADSR for the instrument
ampeg_decay_oncc46=20
ampeg_sustain=0 ampeg_sustain_oncc53=100
ampeg_release_oncc56=6

amp_random=-3 //volume is randomised within a span of 3 decibels

pitch_random=1200 //pitch is randomised within the span of one octave, 2400 for two octaves, -1200 for octave down, etc

pitchlfo_freq=5 //vibrato speed and depth
pitchlfo_depth=5


<group>
sample=piano.wav

transpose=-12 //transposes your initial audio sample, good if your sample is a high pitched note like violin or flute

<region>
delay_random=0 //first note is generated right when note is pressed
<region>
delay_random=50 //further notes are generated at a random point within a span of 50 seconds
